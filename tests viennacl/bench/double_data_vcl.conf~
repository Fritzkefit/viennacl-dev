#set terminal png truecolor
set terminal pngcairo
set autoscale
set key left
set style data lines
set grid
set xlabel "matrix size NxNxN"
set ylabel "GFLOPS"

# colors for viennacl
set style line 4 lt 2 lc rgb "#41b6c4" lw 3 
set style line 3 lt 2 lc rgb "#1d91c0" lw 3 
set style line 2 lt 2 lc rgb "#225ea8" lw 3 
set style line 1 lt 2 lc rgb "#253494" lw 3

# colors for eigen
set style line 8 lt 2 lc rgb "#fd8d3c" lw 2 
set style line 7 lt 2 lc rgb "#fc4e2a" lw 2 
set style line 6 lt 2 lc rgb "#e31a1c" lw 2 
set style line 5 lt 2 lc rgb "#bd0026" lw 2

# colors for openblas
set style line 12 lt 2 lc rgb "#66c2a4" lw 2 
set style line 11 lt 2 lc rgb "#41ae76" lw 2 
set style line 10 lt 2 lc rgb "#238b45" lw 2 
set style line  9 lt 2 lc rgb "#006d2c" lw 2

set output "benchmark_vcl_double.png"

# plot all of the double_data
plot "double_data" using 1:2  title "ViennaCL_NN"     ls  1,         \
     "double_data" using 1:3  title "ViennaCL_NT"     ls  2,         \
     "double_data" using 1:4  title "ViennaCL_TN"     ls  3,         \
     "double_data" using 1:5  title "ViennaCL_TT"     ls  4,         \
     "double_data" using 1:6  title "ViennaCl_AVX_NN" ls  5,         \
     "double_data" using 1:7  title "ViennaCl_AVX_NT" ls  6,         \
     "double_data" using 1:8  title "ViennaCl_AVX_TN" ls  7,         \
     "double_data" using 1:9  title "ViennaCl_AVX_TT" ls  8,         \
#     "double_data" using 1:10 title "ViennaCl_AVX_NN" ls  9,         \
#     "double_data" using 1:11 title "ViennaCl_AVX_NT" ls 10,         \
#     "double_data" using 1:12 title "ViennaCl_AVX_TN" ls 11,         \
#     "double_data" using 1:13 title "ViennaCl_AVX_TT" ls 12

# plot A*B
set output "benchmark_vcl_double_NN.png"
plot "double_data" using 1:2 title "ViennaCL_NN" ls 1, \
     "double_data" using 1:6 title "ViennaCL_AVX_NN" ls 5, \

# plot A*B^T
set output "benchmark_vcl_double_NT.png"
plot "double_data" using 1:3 title "ViennaCL_NT" ls 2, \
     "double_data" using 1:7 title "ViennaCL_AVX_NT" ls 6, \

# plot A^T*B
set output "benchmark_vcl_double_TN.png"
plot "double_data" using 1:4 title "ViennaCL_TN" ls 3, \
     "double_data" using 1:8 title "ViennaCL_AVX_TN" ls 7, \

# plot A^T*B^T
set output "benchmark_vcl_double_TT.png"
plot "double_data" using 1:5 title "ViennaCL_TT" ls 4, \
     "double_data" using 1:9 title "ViennaCL_AVX_TT" ls 8, \
